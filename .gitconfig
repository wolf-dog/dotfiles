[user]
    name = Takaaki Hirano
    email = hivedark@gmail.com
[color]
    ui = auto
[core]
    editor = vim
    pager = less -R
    excludesfile = ~/.gitignore
    whitespace = trailing-space, space-before-tab
    autocrlf = input
    safecrlf = true
    preloadindex = true
    fscache = true
[alias]
    a = add
    aa = add --all :/
    b = branch
    bd = branch -d
    ci = commit
    ca = commit --amend
    co = checkout
    cb = checkout -b
    d = diff
    dc = diff --cached
    dh = diff HEAD^
    f = fetch
    l = log --oneline
    l5 = log --oneline -n5
    ll = log --decorate
    ls = log --decorate --stat
    m = merge --ff-only
    p = pull --ff-only
    pp = pull --prune
    pu = push -u origin
    r = rebase
    ri = rebase -i
    s = status
    sfp = submodule foreach git pull
    su = submodule update
    stl = stash list
    sta = stash apply
    sts = stash save
    std = stash drop
    unstage = reset -q HEAD --
    discard = checkout --
    forget = !git reset --hard HEAD && git clean -d -f
    obey = !git fetch && git reset --hard FETCH_HEAD
[push]
    default = simple
[credential]
    helper = cache --timeout=300

