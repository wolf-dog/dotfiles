[user]
    name = Takaaki Hirano
    email = hivedark@gmail.com
[color]
    ui = auto
[core]
    editor = vim
    pager = less -R
    excludesfile = ~/.gitignore
    autocrlf = input
[alias]
    a = add
    b = branch
    ci = commit
    co = checkout
    d = diff
    dc = diff --cached
    dh = diff HEAD^
    f = fetch
    l = log --decorate --oneline
    ll = log --decorate --oneline
    m = merge --ff-only
    pm = pull --ff-only
    r = rebase
    st = status
[push]
    default = simple
[credential]
    helper = cache --timeout=300

